@model List<_2233.Models.San>
@{
    Layout = null;
}

<!doctype html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>dang nhap quan li</title>
    <meta charset="utf-8" />
    <link href="~/Content/bootstrap-lumen.css" rel="stylesheet" />
</head>
<body>
    <nav class="navbar navbar-expand-sm bg-success navbar-dark">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" href="~/home/TrangChu">Trang Chủ</a>
            </li>
            <li>
                <a class="nav-link" href="~/home/ThemSan">Thêm Sân Bóng</a>
            </li>
            <li>
                <a class="nav-link" href="~/home/SuaSan">Sửa Sân Bóng</a>
            </li>
            <li>
                <a class="nav-link" href="~/home/DongTatCaSan">Đóng Cửa</a>
            </li>
        </ul>
    </nav>
    
    <div>
        <h4>Thông Tin Sân Bóng Hiện Có:</h4>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <td>Mã Sân</td>
                    <td>Giá Tiền Thuê 1 Giờ</td>
                    <td>Tình Trạng</td>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
            {
                <tr>
                    <td>@item.SoSan</td>
                    <td>@item.GiaThue1Gio</td>
                    <td>@item.TinhTrang</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</body>
</html>


